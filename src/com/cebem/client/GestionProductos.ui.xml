<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel>
			<g:Label text="Gestión producto"/>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label text="Nombre:"/>
			<g:TextBox ui:field="nombre"/>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label text="Tipo:"/>
			<g:ListBox ui:field="tipo"/>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label text="Precio:"/>
			<g:DoubleBox ui:field="precio"/>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Button text="Añadir" ui:field="add"/>
			<g:Button text="Borrar seleccionado" ui:field="borrar"/>
			<g:Button text="Modificar seleccionado" ui:field="modificar"/>
			<g:Button text="Borrar formulario" ui:field="borrarFormulario"/>
			<g:Button text="Elevar al cuadrado en el cliente" ui:field="button"/>
			<g:Button text="Elevar al cuadrado en el servidor" ui:field="button_1"/>
			<g:Button ui:field="ir">ir</g:Button>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<p1:CellList ui:field="cellList"/>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 